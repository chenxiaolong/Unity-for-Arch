-                .type   = XA_INTEGER,
-                .format = 32,
-        };
-        gsize nvalues;
-
-        property.data.i = g_variant_get_fixed_array (value, &nvalues, sizeof (gint32));
-        g_variant_unref (value);
-
-        if (nvalues != 4) {
-                g_error ("Pressurecurve requires 4 values.");
-                return;
-        }
-
-        wacom_set_property (device, &property);
