-static XDevice *
-open_device (GsdWacomDevice *device)
-{
-	XDevice *xdev;
-	int id;
-
-	id = get_device_id (device);
-	if (id < 0)
-		return NULL;
-
-	gdk_error_trap_push ();
-	xdev = XOpenDevice (GDK_DISPLAY_XDISPLAY (gdk_display_get_default ()), id);
-	if (gdk_error_trap_pop () || (xdev == NULL))
-		return NULL;
-
-	return xdev;
-}
