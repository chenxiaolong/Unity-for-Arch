-static void
-effects_slider_set_value (GtkRange *slider, GSettings *settings)
-{
-    gchar **keys;
-    gdouble red, green, blue;
-    gdouble value;
-
-    keys = g_object_get_data (G_OBJECT (slider), "settings-keys");
