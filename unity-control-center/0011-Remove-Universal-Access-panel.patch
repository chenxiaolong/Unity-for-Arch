-static gboolean
-get_large_text_mapping (GValue   *value,
-                        GVariant *variant,
-                        gpointer  user_data)
-{
-  gdouble factor;
-  gboolean large;
-
-  factor = g_variant_get_double (variant);
-  large = factor > DPI_FACTOR_NORMAL;
-  g_value_set_boolean (value, large);
-
-  return TRUE;
-}
